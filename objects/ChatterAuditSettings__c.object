<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Setup data for Chatter Compliance app</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Active__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Indicates that the batch process is currently running</description>
        <externalId>false</externalId>
        <label>Active</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ArchivedPMessagesJobId__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Archived Private Messages Job Id</label>
        <length>20</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ArchivedPMessagesUpdateFreq__c</fullName>
        <defaultValue>12</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Archived Private Messages Update Freq</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ArchivedPMessagesUpdateLastExecution__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Archived Private Messages Update Last Ex</label>
        <required>false</required>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>ChatterAuditOwnerId__c</fullName>
        <deprecated>false</deprecated>
        <description>SFDC record Id for the user designated as owner for all chatter compliance records</description>
        <externalId>false</externalId>
        <label>Chatter Compliance OwnerId</label>
        <length>18</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ChatterAuditUpdateFequency__c</fullName>
        <deprecated>false</deprecated>
        <description>Set the batch process frequency.Any number from 1 to 23 hours.</description>
        <externalId>false</externalId>
        <label>Batch Update Frequency</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ContentVersionDeleteBlocker__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Protect files attached to a chatter conversation by setting this field ON</inlineHelpText>
        <label>Content Version Delete Blocker</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>PreviousChatterDataLoaded__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Previous Chatter Data Loaded</label>
        <type>Checkbox</type>
    </fields>
    <label>ChatterComplianceSettings</label>
</CustomObject>
